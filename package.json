{
  "name": "task-manager_frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lintOld": "prettier --write ./src && next lint",
    "lint": "npm run lint:tsc && npm run lint:eslint && npm run lint:prettier",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint ./src --ext .js,.ts,.tsx",
    "lint:prettier": "prettier --check ./src",
    "fix": "npm run fix:prettier && npm run fix:eslint",
    "fix:prettier": "prettier --write ./src",
    "fix:eslint": "eslint ./src --fix --ext .js,.ts,.tsx",
    "schema:generate": "graphql-code-generator --config ./codegen.ts"
  },
  "dependencies": {
    "@ant-design/icons": "^5.1.4",
    "@reduxjs/toolkit": "^1.9.3",
    "@rtk-query/graphql-request-base-query": "^1.0.3",
    "@types/node": "18.15.3",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "antd": "^5.6.1",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "graphql": "^16.6.0",
    "graphql-request": "^6.1.0",
    "next": "13.2.4",
    "next-auth": "^4.22.1",
    "next-redux-wrapper": "^8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.5",
    "recharts": "^2.7.1",
    "sass": "^1.59.3",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/typescript": "^4.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.0",
    "@graphql-codegen/typescript-rtk-query": "^2.4.1",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "prettier": "^2.8.8"
  }
}
